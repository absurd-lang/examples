if true {
    print(true);
}

if false {
    print(false);
} else {
    print(true);
}

if false {
    print(false);
} elif true {
    print(true);
} else {
    print(false);
}

while true {
    print("while loop");
    break;
}

loop {
    print("loop");
    break;
}

loop 3 {
    print("loop 3 times");
    break;
}

for item in [1, 2, 3] {
    print(item);
}

for item, index in [1, 2, 3] {
    print(index);
}

for item in 1..3 {
    print(item);
}
